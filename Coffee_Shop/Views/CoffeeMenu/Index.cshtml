@using EntityLayer.Concrete
@model List<CoffeeMenu>
<style>
	h2 {
		color: white;
		
	}
</style>
<h2>Hoşgeldiniz @TempData["UserName"]</h2>

<div class="row">
	<div class="col-md-12">
